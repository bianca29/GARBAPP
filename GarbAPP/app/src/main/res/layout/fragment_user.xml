            android:text="TextView"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/Distrito"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Distrito"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
